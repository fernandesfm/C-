//Compare as palavras "texto" e "TEXTO" para ver se elas s√£o iguais, de uma forma que o resultado seja falso.

using System;

namespace Exercicio5
{
    class Ex5
    {
        static void Main(string[] args)
        {
            string str1 = "texto";
            string str2 = "TEXTO";

            if(String.Compare(str1, str2) != 0)
            {
                Console.WriteLine("As strings comparadas sao diferentes");
            }
        }
    }
}
